<?php 
interface Kendaraaninterface{
  public function get_bahan_bakar();
}
 
class MOTOR implements KendaraanInterface{
  public function get_bahan_bakar()
  {
    return 'bahan bakar minyak';
  }
}
 
class MOBIL implements KendaraanInterface{
  public function get_bahan_bakar()
  {
    return 'bahan bakar minyak';
  }
}
 
class BUS implements KendaraanInterface{
  public function get_bahan_bakar()
  {
    return 'Solar';
  }
}
?>
